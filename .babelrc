{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "iOS >= 8.0",
          "Android >= 4.4",
          "Chrome >= 30",
          "Safari >= 9",
          "Firefox ESR",
          "Opera 12.1"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
